{
  "name": "react-component",
  "version": "1.0.0",
  "description": "A React UI Component library for web",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development start-storybook -p 8080 -c .storybook",
    "eslint:fix": "eslint './components/**/*.jsx' --fix",
    "lint": "eslint --ext .js,.jsx components && stylelint components/**/*.css.js",
    "pre-commit": "npm run lint-staged",
    "lint-staged": "lint-staged",
    "test": "jest --config jest.config.js",
    "test:update": "jest -u",
    "format": "prettier --write ./components/**/*.js ./components/**/*.jsx"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/front-end-captain/react-component.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/front-end-captain/react-component/issues"
  },
  "homepage": "https://github.com/front-end-captain/react-component#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/react": "^4.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "power-assert": "^1.6.1",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "scroll-into-view-if-needed": "^2.2.20",
    "styled-components": "^4.1.3",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-processor-styled-components": "^1.5.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react-icons": "^3.2.2"
  }
}
